@model List<GetAppVersions.Models.AppVersionInfo>

<span>
    <div style='text-align:center;margin-bottom:10px;'>
        <!--<span style='float:left;'></span>-->
        <span><img src='/content/@(ViewBag.isZzk?"zzk":"csm").png'  style='width:40px;'/></span>
        <span style="font-size:30px;">@(ViewBag.isZzk?"中转库":"车商猫")Android版APP各商店版本信息</span>
    </div>
    <table style="width:50%;text-align:center;margin:0 auto;" border="5">
        <tr style='background-color:#636363;color:#FFFFFF'>
            <td>ID</td>
            <td>商店</td>
            <td>当前版本</td>
            <td>URL</td>
        </tr>
        @{int i=0;}   
        @foreach(var m in Model)
        {
           string strColor = i%2 != 0?"#B2DFEE":"";
          <tr style="background-color:@strColor">
            <td>@m.Id</td>
            <td>@m.StoreName</td>
            <td>@m.Version</td>
            <td><a href="@m.Url" target="_blank">点击跳转</a></td>
          </tr>
          i++;
        }
    </table>
    <div style='text-align:center;margin-top:15px;'>
        <a href='/@(ViewBag.isZzk?"":"zzk")'>查看@(ViewBag.isZzk?"车商猫":"中转库")</a>
    </div>
</span>
